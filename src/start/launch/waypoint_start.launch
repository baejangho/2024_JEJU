<?xml version="1.0"?>
<launch>

    <!--start launch list-->
    <include file="$(find gps_mbc)/launch/mbc.launch"/>
    <!--include file="$(find rplidar_ros)/launch/rplidar_a1.launch"/-->
    
    <!-- Set permissions for serial ports -->
    <node pkg="rosserial_python" type="serial_node.py" name="serial_node" args="/dev/ttyACM0" output="screen">
        <param name="port" value="/dev/ttyACM0"/>
        <param name="baud" value="115200"/>
    </node>

    <node pkg="joy" type="joy_node" name="joy_node" output="screen"/>
    
    <!--node pkg="start" type="joy_act.py" name="joy_act_node" output="screen"/-->
    <node pkg="start" type="waypoint_pick.py" name="waypoint_pick_node" output="screen"/>
    <node pkg="start" type="waypoint_mbc.py" name="waypoint_node" output="screen"/>
    <!--node pkg="start" type="pid.py" name="pid_node" output="screen"/-->
    
</launch>

